<h1>Articles</h1>
<ul class="list-group">
  <% @articles.each do |article| %>
    <li class = "list-group-item"><%= link_to article.title, article_path(article) %> <%= link_to "Delete",
            article_path(article),
            method: :delete,
            data: { confirm: "Are you sure?" } %>
    </li>
  <% end %>
</ul>
